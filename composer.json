{
  "name": "spryker/merchant-product-offer-search",
  "type": "library",
  "description": "MerchantProductOfferSearch module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/event-behavior": "^1.8.0",
    "spryker/kernel": "^3.30.0",
    "spryker/merchant": "^2.0.0",
    "spryker/merchant-profile": "^0.1.0",
    "spryker/product": "^6.0.0",
    "spryker/product-offer": "^0.1.0",
    "spryker/product-page-search": "^3.2.0",
    "spryker/product-page-search-extension": "^1.0.0",
    "spryker/propel-orm": "^1.0.0"
  },
  "require-dev": {
    "spryker/category": "*",
    "spryker/event": "*",
    "spryker/locale": "*",
    "spryker/merchant-product-offer": "*",
    "spryker/product-attribute": "*",
    "spryker/product-category": "*",
    "spryker/product-search": "*",
    "spryker/propel": "*",
    "spryker/queue": "*",
    "spryker/store": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/event": "To use event behavior.",
    "spryker/merchant-product-offer": "Required for using the Listeners"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
